{
    "version": 1,
    "routes": [
      {
        "src": "/_next/static/(.*)",
        "dest": "/_next/static/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/_next/data/(.*)",
        "dest": "/_next/data/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }